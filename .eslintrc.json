{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier", "react"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ],
    "radix": "off"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
